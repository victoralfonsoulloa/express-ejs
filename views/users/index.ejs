<%- include('../partials/header') %>
    <h1><%=title%></h1>
      <div class="container">
    <table class="table table-hover table-striped">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(function(user){ %>
                <tr>
                    <td><a href="users/view/<%= user.id %>"><%= user.first_name %></a></td>
                    <td><%= user.last_name %></td>
                    <td><%= user.email %></td>
                </tr>
            <% }); %>

        </tbody>
    </table>
</div>
<%- include('../partials/footer') %>